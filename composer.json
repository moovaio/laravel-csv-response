{
    "name": "moova/neoxia-laravel-csv-response",
    "license": "MIT",
    "description": "Add a CSV response type to Laravel (moova.io fork for neoxia/laravel-csv-response)",
    "require": {
        "php": ">=5.5.9",
        "illuminate/routing": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "1.3.*",
        "php-coveralls/php-coveralls": "2.2.*"
    },
    "autoload": {
         "psr-4": {
            "Neoxia\\": "src/Neoxia/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Neoxia\\Routing\\ResponseFactoryServiceProvider"
            ]
        }
    }
}
